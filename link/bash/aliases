alias b="bundle exec"

# check for presence of hub, if so use it instead of git
type hub >/dev/null 2>&1 && {
  alias git="hub"
}

alias g="git"
alias gs="git status"
alias gp="git push"
alias gd="git diff"
alias gl="git pull"
alias gb="git branch"
alias gco="git checkout"
alias rc="rails console"
alias rs="rails server"
alias rg="rails generate"
alias r="rails"
